{
  "name": "smth",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "tsc -b && vite build",
    "postbuild": "react-snap && node scripts/cleanup.js",
    "deploy:preview": "git checkout develop && git pull origin develop && npm run build && wrangler pages deploy dist --project-name=smth-fun",
    "deploy:prod": "git checkout master && git pull origin master && npm run build && wrangler pages deploy dist --project-name=smth-fun",
    "lint": "eslint .",
    "preview": "vite preview",
    "analyze": "npx vite-bundle-visualizer"
  },
  "reactSnap": {
    "source": "dist"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fontsource/roboto": "^5.2.9",
    "@mui/icons-material": "^7.3.6",
    "@mui/material": "^7.3.6",
    "better-react-mathjax": "^2.3.0",
    "i18next": "^25.7.3",
    "mathjs": "^15.1.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-helmet": "^6.1.0",
    "react-i18next": "^16.5.1",
    "react-router": "^7.13.0",
    "react-snap": "^1.23.0",
    "vite-plugin-sitemap": "^0.8.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@types/react-helmet": "^6.1.11",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "vite-react-ssg": "^0.8.9"
  }
}
